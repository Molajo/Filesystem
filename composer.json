{
    "name": "molajo/filesystem",
    "type": "molajo-package",
    "description": "Simple, uniform file and directory services API for PHP applications that enables applications to interact with any filesystem for which an adapter has been defined in a common way.",
    "keywords": ["Filesystem", "CommonApi", "Molajo"],
    "homepage": "http://github.com/Molajo/Filesystem",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "commonapi/filesystem": "dev-master",
        "commonapi/exception": "dev-master"
    },

    "support": {
        "event": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },
    "autoload": {
        "psr-4": {
            "Molajo\\Filesystem\\": "Source/"
        }
    },

    "archive": {
        "exclude": ["/.dev"]
    },

    "minimum-stability": "dev"
}
