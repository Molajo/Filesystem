<phpunit bootstrap="./Autoload.php">
    <testsuites>
        <testsuite name="Integration">
            <directory>./tests/Integration/</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory>./src/</directory>
        </whitelist>
    </filter>
    <logging>
        <log type="junit"
             target="Build/logs/junit.xml"
             logIncompleteSkipped="false"
                />
    </logging>
</phpunit>
